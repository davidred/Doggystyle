<h2>Sign Up!</h2>

<form action="<%= users_url %>" method="POST">
  <%= form_auth %>
  <label for="username">Username</label>
  <input type="text"
         id="username"
         name="user[username]">
  <br>
  <br>
  <label for="password">Password</label>
  <input type="password"
         id="password"
         name="user[password]">

  <br>
  <br>
  <input type="submit" value="Sign Up">

</form>