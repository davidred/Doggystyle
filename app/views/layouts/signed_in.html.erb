<!DOCTYPE html>
<html>
<head>
  <title>Doggystyle</title>
  <%= stylesheet_link_tag 'signed_in', media: 'all' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

</head>
<body>

  <nav class="signed-in-nav">
    <ul class="signed-in-nav-list group">
      <li>
        <%= link_to current_user.username, user_url(current_user) %>
      </li>
      <li>
        <%= button_to "Sign out", session_url, method: :delete %>
      </li>
    </ul>
  </nav>
  <main>
    <section class="sidebar">
      <figure class="sidebar-logo">
        <img src="<%= asset_path 'Doggystyle-logo.png' %>">
      </figure>
      <ul class="sidebar-list">
        <li><%= link_to "Browse Matches", users_url %></li>
        <li><%= link_to "Messages", inbox_url %></li>
        <li><%= link_to "Visitors", visits_url %></li>
      </ul>
    </section>
    <section class="page-content">
      <!-- <nav class="content-nav"></nav> -->
      <%= yield %>

    </section>
  </main>



</body>
